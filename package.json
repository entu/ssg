    "js-yaml": "4.1.0",
    "jstransformer-markdown-it": "2.1.0",
    "jstransformer-stylus": "1.5.0",
    "klaw": "4.0.1",
    "lodash": "4.17.21",
    "markdown-it": "12.3.2",
    "markdown-it-attrs": "4.1.3",
    "markdown-it-multimd-table": "4.1.3",
    "markdown-it-sup": "1.0.0",
    "pug": "3.0.2",
    "request": "2.88.2",
    "stylus": "0.56.0",
    "uglify-js": "3.15.0"
  }
