{
  "name": "entu-ssg",
  "productName": "Entu SSG",
  "version": "5.3.2",
  "author": "Argo Roots <argo@roots.ee>",
  "description": "Simple Pug (Jade), Markdown, Yaml static site generator",
  "keywords": [
    "ssg",
    "cms",
    "static",
    "html",
    "jade",
    "pug",
    "markdown",
    "yaml"
  ],
  "homepage": "https://entu.site",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entu/ssg.git"
  },
  "bugs": {
    "url": "https://github.com/entu/ssg/issues"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "node src/build.js",
    "serve": "node src/serve.js",
    "docs": "node src/build.js docs/source/config.yaml full"
  },
  "bin": {
