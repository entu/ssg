    "html-minifier": "4.0.0",
    "js-yaml": "4.1.0",
    "jstransformer-markdown-it": "3.0.0",
    "jstransformer-stylus": "1.5.0",
    "klaw": "4.1.0",
    "lodash": "4.17.21",
    "markdown-it": "14.1.0",
    "markdown-it-attrs": "4.3.1",
    "markdown-it-multimd-table": "4.2.3",
    "markdown-it-sup": "2.0.0",
    "mime-types": "3.0.1",
    "pug": "3.0.3",
    "stylus": "0.64.0",
    "uglify-js": "3.19.3"
